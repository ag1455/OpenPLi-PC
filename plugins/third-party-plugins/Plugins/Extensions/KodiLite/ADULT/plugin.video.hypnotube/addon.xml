<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.hypnotube"
	   name="hypnotube"
	   version="1.0.1"
	   provider-name="linuxsat-support">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
	   
  <extension point="xbmc.python.pluginsource"
			 library="default.py">
    	<provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Adult(18+) videos from hypnotube.com</summary>
    <description>Adult(18+) videos from hypnotube.com</description>
    <platform>all</platform>
  </extension>
</addon>








